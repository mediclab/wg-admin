# Server
[Interface]
PrivateKey = {SERVER_PRIVATE_KEY}
Address = {SERVER_ADDRESS}/24
ListenPort = {SERVER_PORT}
PostUp = iptables -t nat -A POSTROUTING -s {SERVER_ADDRESS}/24 -o eth0 -j MASQUERADE; iptables -A INPUT -p udp -m udp --dport {SERVER_PORT} -j ACCEPT; iptables -A FORWARD -i {INTERFACE} -j ACCEPT; iptables -A FORWARD -o {INTERFACE} -j ACCEPT;
PostDown =
